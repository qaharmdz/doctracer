{
    "name": "qaharmdz/doctracer",
    "description": "DocTracer helps you to create meaningful API documentation reports. Reflecting PHP class information and show markdown formatted documentation on one page.",
    "type": "library",
    "keywords": ["php","docblocks"],
    "license": "MIT",
    "authors": [
        {
          "name": "Mudzakkir",
          "email": "me@mdzstack.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "michelf/php-markdown": "^1.9"
    },
    "require-dev": {
        "kint-php/kint": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Mdz\\": "src/"
        }
    },
    "prefer-stable": true,
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
